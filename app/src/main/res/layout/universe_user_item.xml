<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/universe_user_item_background"
        android:layout_width="393dp"
        android:layout_height="77dp"
        android:background="#00FFFFFF"
        android:baselineAlignBottom="false"
        android:scaleType="fitStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024"
        app:srcCompat="@drawable/list_item"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <TextView
        android:id="@+id/universe_user_username"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:fontFamily="@font/aldrich"
        android:textAlignment="viewStart"
        android:textColor="@color/pink"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/universe_user_item_background"
        app:layout_constraintEnd_toEndOf="@+id/universe_user_item_background"

        app:layout_constraintHorizontal_bias="0.134"
        app:layout_constraintStart_toStartOf="@+id/universe_user_item_background"
        app:layout_constraintTop_toTopOf="@+id/universe_user_item_background"
        app:layout_constraintVertical_bias="0.574"
        tools:ignore="TextContrastCheck"
        tools:text="" />

</androidx.constraintlayout.widget.ConstraintLayout>